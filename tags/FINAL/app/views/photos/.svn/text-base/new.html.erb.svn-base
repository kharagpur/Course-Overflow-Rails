<h1>Photos#new</h1>
<p>Find me in app/views/photos/new.html.erb</p>
<% form_for(:photo, :url=>{:controller => 'photos', :action=>'create'}, :html=>{:multipart=>true}) do |f| %>
	<%= render :partial =>"form", :locals => {:f => f }%> 
	<%= f.hidden_field :topic_id %> #:value => @topic.id 
	<div class = "popup" >
			<div class = "popup_content" >
				<%= submit_tag ("Add New Photo")%>	
			</div>
	</div>
<% end %>
